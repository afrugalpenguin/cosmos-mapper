{
  "output": "./output",
  "sampleSize": 100,
  "databases": [],
  "containers": {
    "include": [],
    "exclude": ["*-archive", "*-backup", "test-*"]
  },
  "formats": ["markdown", "html"],
  "typeDetection": {
    "customPatterns": [
      {
        "name": "sku",
        "pattern": "^SKU-\\d{6}$",
        "displayName": "SKU"
      }
    ],
    "enumDetection": {
      "enabled": true,
      "maxUniqueValues": 10,
      "minFrequency": 0.8
    }
  },
  "validation": {
    "enabled": false,
    "sampleSize": 1000,
    "weights": {
      "referentialIntegrity": 0.45,
      "typeConsistency": 0.20,
      "frequency": 0.15,
      "namingPattern": 0.20
    }
  },
  "versioning": {
    "cacheDir": ".cosmoscache",
    "retention": 10,
    "failOnBreaking": false
  }
}
