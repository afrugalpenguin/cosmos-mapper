<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Cosmos DB Schema Documentation</title>
  <script src="https://cdn.tailwindcss.com"></script>
  <script src="https://cdn.jsdelivr.net/npm/mermaid@10/dist/mermaid.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/panzoom@9.4.3/dist/panzoom.min.js"></script>
  <script>
    tailwind.config = {
      theme: {
        extend: {
          colors: {
            cosmos: {
              50: '#eff6ff',
              100: '#dbeafe',
              200: '#bfdbfe',
              500: '#3b82f6',
              600: '#2563eb',
              700: '#1d4ed8',
              800: '#1e40af',
              900: '#1e3a8a',
            }
          }
        }
      }
    }
  </script>
  <style>
    .mermaid { background: transparent; }
    details summary { cursor: pointer; list-style: none; }
    details summary::-webkit-details-marker { display: none; }
    details summary::marker { display: none; }
    details[open] > summary .chevron { transform: rotate(90deg); }
    .chevron { transition: transform 0.2s; transform-origin: center; }
    .diagram-container {
      position: relative;
      overflow: hidden;
      min-height: 400px;
      border: 1px solid #e5e7eb;
      border-radius: 0.5rem;
      background: #f9fafb;
    }
    .diagram-viewport {
      width: 100%;
      height: 100%;
      cursor: grab;
    }
    .diagram-viewport:active { cursor: grabbing; }
    .diagram-controls {
      position: absolute;
      top: 0.5rem;
      right: 0.5rem;
      display: flex;
      gap: 0.25rem;
      z-index: 10;
    }
    .diagram-controls button {
      width: 2rem;
      height: 2rem;
      display: flex;
      align-items: center;
      justify-content: center;
      background: white;
      border: 1px solid #d1d5db;
      border-radius: 0.375rem;
      cursor: pointer;
      font-weight: bold;
      color: #374151;
      transition: all 0.15s;
    }
    .diagram-controls button:hover {
      background: #f3f4f6;
      border-color: #9ca3af;
    }
    /* Search highlighting for relationships */
    .relationship-badge.search-match {
      background: #fef08a !important;
      border-color: #fbbf24 !important;
      color: #92400e !important;
    }
    /* Sidebar styles */
    .sidebar {
      position: fixed;
      left: 0;
      top: 0;
      height: 100vh;
      width: 280px;
      background: white;
      border-right: 1px solid #e5e7eb;
      overflow-y: auto;
      z-index: 40;
      transform: translateX(-100%);
      transition: transform 0.3s ease;
    }
    .sidebar.open { transform: translateX(0); }
    .sidebar-overlay {
      position: fixed;
      inset: 0;
      background: rgba(0, 0, 0, 0.3);
      z-index: 30;
      display: none;
    }
    .sidebar-overlay.open { display: block; }
    @media (min-width: 1024px) {
      .sidebar {
        transform: translateX(0);
        top: 76px;
        height: calc(100vh - 76px);
      }
      .sidebar-overlay { display: none !important; }
      .main-content { margin-left: 280px; }
    }
    /* Search highlight */
    .search-highlight {
      background: #fef08a;
      padding: 0 2px;
      border-radius: 2px;
    }
    /* Confidence badges */
    .confidence-badge {
      display: inline-flex;
      align-items: center;
      padding: 0.125rem 0.375rem;
      border-radius: 9999px;
      font-size: 0.65rem;
      font-weight: 600;
      margin-left: 0.25rem;
    }
    .confidence-high {
      background: #dcfce7;
      color: #166534;
      border: 1px solid #86efac;
    }
    .confidence-medium {
      background: #fef9c3;
      color: #854d0e;
      border: 1px solid #fde047;
    }
    .confidence-low {
      background: #fee2e2;
      color: #991b1b;
      border: 1px solid #fca5a5;
    }
    .confidence-very-low {
      background: #f3f4f6;
      color: #6b7280;
      border: 1px solid #d1d5db;
    }
    .search-match { display: block !important; }
    .search-hidden { display: none !important; }
    /* Nav active state */
    .nav-item.active {
      background: #dbeafe;
      border-left-color: #3b82f6;
    }
  </style>
</head>
<body class="bg-gray-50 min-h-screen">
  <!-- Header -->
  <header class="bg-gradient-to-r from-cosmos-700 to-cosmos-900 text-white shadow-lg sticky top-0 z-20">
    <div class="px-4 py-4 sm:px-6 lg:px-8">
      <div class="flex items-center justify-between gap-4">
        <div class="flex items-center gap-4">
          <!-- Mobile menu button -->
          <button onclick="toggleSidebar()" class="lg:hidden p-2 hover:bg-white/10 rounded-lg">
            <svg class="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 6h16M4 12h16M4 18h16"/>
            </svg>
          </button>
          <div class="bg-white/10 p-2 rounded-lg hidden sm:block">
            <svg class="w-6 h-6" fill="currentColor" viewBox="0 0 24 24">
              <path d="M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm-2 15l-5-5 1.41-1.41L10 14.17l7.59-7.59L19 8l-9 9z"/>
            </svg>
          </div>
          <div>
            <h1 class="text-lg sm:text-xl font-bold">Cosmos DB Schema Documentation</h1>
            <p class="text-cosmos-200 text-xs sm:text-sm hidden sm:block">Generated: <%= timestamp %></p>
          </div>
        </div>
        <!-- Search -->
        <div class="flex-1 max-w-md">
          <div class="relative">
            <input
              type="text"
              id="searchInput"
              placeholder="Search containers, properties, relationships..."
              class="w-full px-4 py-2 pl-10 text-sm text-gray-900 bg-white/90 rounded-lg focus:outline-none focus:ring-2 focus:ring-white/50"
              onkeyup="handleSearch(this.value)"
            >
            <svg class="absolute left-3 top-2.5 w-4 h-4 text-gray-500" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M21 21l-6-6m2-5a7 7 0 11-14 0 7 7 0 0114 0z"/>
            </svg>
            <button onclick="clearSearch()" id="clearSearchBtn" class="absolute right-3 top-2.5 text-gray-500 hover:text-gray-700 hidden">
              <svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M6 18L18 6M6 6l12 12"/>
              </svg>
            </button>
          </div>
          <div id="searchResults" class="mt-1 text-xs text-white/80 hidden"></div>
        </div>
      </div>
    </div>
  </header>

  <!-- Sidebar Overlay (mobile) -->
  <div class="sidebar-overlay" id="sidebarOverlay" onclick="toggleSidebar()"></div>

  <!-- Sidebar -->
  <aside class="sidebar" id="sidebar">
    <div class="p-4 border-b border-gray-200 bg-gray-50">
      <div class="flex items-center justify-between">
        <h2 class="font-semibold text-gray-700">Navigation</h2>
        <div class="flex gap-1">
          <button onclick="expandAll()" class="p-1.5 text-xs text-gray-600 hover:bg-gray-200 rounded" title="Expand all">
            <svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 8V4m0 0h4M4 4l5 5m11-1V4m0 0h-4m4 0l-5 5M4 16v4m0 0h4m-4 0l5-5m11 5l-5-5m5 5v-4m0 4h-4"/>
            </svg>
          </button>
          <button onclick="collapseAll()" class="p-1.5 text-xs text-gray-600 hover:bg-gray-200 rounded" title="Collapse all">
            <svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 9V4.5M9 9H4.5M9 9L3.75 3.75M9 15v4.5M9 15H4.5M9 15l-5.25 5.25M15 9h4.5M15 9V4.5M15 9l5.25-5.25M15 15h4.5M15 15v4.5m0-4.5l5.25 5.25"/>
            </svg>
          </button>
        </div>
      </div>
    </div>
    <nav class="p-2">
      <a href="#overview" class="nav-item flex items-center gap-2 px-3 py-2 text-sm text-gray-700 hover:bg-gray-100 rounded-lg border-l-2 border-transparent">
        <svg class="w-4 h-4 text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 19v-6a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2a2 2 0 002-2zm0 0V9a2 2 0 012-2h2a2 2 0 012 2v10m-6 0a2 2 0 002 2h2a2 2 0 002-2m0 0V5a2 2 0 012-2h2a2 2 0 012 2v14a2 2 0 01-2 2h-2a2 2 0 01-2-2z"/>
        </svg>
        Overview
      </a>
      <a href="#erd" class="nav-item flex items-center gap-2 px-3 py-2 text-sm text-gray-700 hover:bg-gray-100 rounded-lg border-l-2 border-transparent">
        <svg class="w-4 h-4 text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 17V7m0 10a2 2 0 01-2 2H5a2 2 0 01-2-2V7a2 2 0 012-2h2a2 2 0 012 2m0 10a2 2 0 002 2h2a2 2 0 002-2M9 7a2 2 0 012-2h2a2 2 0 012 2m0 10V7m0 10a2 2 0 002 2h2a2 2 0 002-2V7a2 2 0 00-2-2h-2a2 2 0 00-2 2"/>
        </svg>
        ERD Diagram
      </a>
      <% for (const [dbName, dbInfo] of Object.entries(databases)) { %>
      <details class="mt-3 nav-db-section">
        <summary class="flex items-center gap-2 px-3 py-1.5 text-xs font-semibold text-gray-500 uppercase tracking-wider cursor-pointer hover:bg-gray-100 rounded-lg">
          <svg class="w-3 h-3 chevron text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"/>
          </svg>
          <%= dbName %>
          <span class="text-gray-400 font-normal normal-case">(<%= dbInfo.containers.length %>)</span>
        </summary>
        <div class="ml-2">
          <% for (const containerName of dbInfo.containers) { %>
          <a href="#<%= dbName %>-<%= containerName %>"
             class="nav-item flex items-center gap-2 px-3 py-1.5 text-sm text-gray-600 hover:bg-gray-100 rounded-lg border-l-2 border-transparent ml-2"
             data-container="<%= containerName %>"
             data-database="<%= dbName %>">
            <svg class="w-3 h-3 text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12h6m-6 4h6m2 5H7a2 2 0 01-2-2V5a2 2 0 012-2h5.586a1 1 0 01.707.293l5.414 5.414a1 1 0 01.293.707V19a2 2 0 01-2 2z"/>
            </svg>
            <%= containerName %>
          </a>
          <% } %>
        </div>
      </details>
      <% } %>
    </nav>
  </aside>

  <main class="main-content px-4 py-8 sm:px-6 lg:px-8">
    <!-- Stats Cards -->
    <section id="overview">
      <div class="grid grid-cols-1 md:grid-cols-3 gap-6 mb-8">
        <div class="bg-white rounded-xl shadow-sm border border-gray-200 p-6">
          <div class="flex items-center">
            <div class="p-3 bg-blue-100 rounded-lg">
              <svg class="w-6 h-6 text-blue-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 7v10c0 2.21 3.582 4 8 4s8-1.79 8-4V7M4 7c0 2.21 3.582 4 8 4s8-1.79 8-4M4 7c0-2.21 3.582-4 8-4s8 1.79 8 4"/>
              </svg>
            </div>
            <div class="ml-4">
              <p class="text-sm font-medium text-gray-500">Databases</p>
              <p class="text-2xl font-bold text-gray-900"><%= Object.keys(databases).length %></p>
            </div>
          </div>
        </div>
        <div class="bg-white rounded-xl shadow-sm border border-gray-200 p-6">
          <div class="flex items-center">
            <div class="p-3 bg-green-100 rounded-lg">
              <svg class="w-6 h-6 text-green-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 11H5m14 0a2 2 0 012 2v6a2 2 0 01-2 2H5a2 2 0 01-2-2v-6a2 2 0 012-2m14 0V9a2 2 0 00-2-2M5 11V9a2 2 0 012-2m0 0V5a2 2 0 012-2h6a2 2 0 012 2v2M7 7h10"/>
              </svg>
            </div>
            <div class="ml-4">
              <p class="text-sm font-medium text-gray-500">Containers</p>
              <p class="text-2xl font-bold text-gray-900"><%= Object.keys(containerSchemas).length %></p>
            </div>
          </div>
        </div>
        <div class="bg-white rounded-xl shadow-sm border border-gray-200 p-6">
          <div class="flex items-center">
            <div class="p-3 bg-purple-100 rounded-lg">
              <svg class="w-6 h-6 text-purple-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13.828 10.172a4 4 0 00-5.656 0l-4 4a4 4 0 105.656 5.656l1.102-1.101m-.758-4.899a4 4 0 005.656 0l4-4a4 4 0 00-5.656-5.656l-1.1 1.1"/>
              </svg>
            </div>
            <div class="ml-4">
              <p class="text-sm font-medium text-gray-500">Relationships</p>
              <p class="text-2xl font-bold text-gray-900"><%= relationships.filter(r => !r.isOrphan).length %></p>
            </div>
          </div>
        </div>
      </div>

      <!-- Validation Summary -->
      <%
        const validRels = relationships.filter(r => !r.isOrphan);
        const relsWithConfidence = validRels.filter(r => r.confidence);
        const validatedCount = relsWithConfidence.filter(r => r.confidence.validated).length;
        const highConf = relsWithConfidence.filter(r => r.confidence.level === 'high').length;
        const medConf = relsWithConfidence.filter(r => r.confidence.level === 'medium').length;
        const lowConf = relsWithConfidence.filter(r => r.confidence.level === 'low').length;
        const veryLowConf = relsWithConfidence.filter(r => r.confidence.level === 'very-low').length;
        const avgScore = relsWithConfidence.length > 0
          ? Math.round(relsWithConfidence.reduce((sum, r) => sum + r.confidence.score, 0) / relsWithConfidence.length)
          : 0;
        const lowConfRels = relsWithConfidence
          .filter(r => r.confidence.level === 'low' || r.confidence.level === 'very-low')
          .sort((a, b) => a.confidence.score - b.confidence.score);
        const orphanRels = relationships.filter(r => r.isOrphan);
      %>
      <% if (relsWithConfidence.length > 0) { %>
      <div class="bg-white rounded-xl shadow-sm border border-gray-200 p-6 mb-8">
        <h2 class="text-lg font-bold text-gray-900 mb-4 flex items-center gap-2">
          <svg class="w-5 h-5 text-indigo-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z"/>
          </svg>
          Relationship Validation Summary
          <% if (validatedCount > 0) { %>
          <span class="ml-2 px-2 py-0.5 text-xs font-medium bg-green-100 text-green-700 rounded-full">
            <%= validatedCount %> validated
          </span>
          <% } %>
        </h2>

        <div class="grid grid-cols-1 lg:grid-cols-2 gap-6">
          <!-- Confidence Distribution -->
          <div>
            <h3 class="text-sm font-medium text-gray-700 mb-3">Confidence Distribution</h3>
            <div class="space-y-2">
              <div class="flex items-center gap-3">
                <span class="w-20 text-xs text-gray-600">High</span>
                <div class="flex-1 bg-gray-200 rounded-full h-4 overflow-hidden">
                  <div class="bg-green-500 h-4 rounded-full transition-all" style="width: <%= relsWithConfidence.length > 0 ? (highConf / relsWithConfidence.length * 100) : 0 %>%"></div>
                </div>
                <span class="w-8 text-xs text-gray-700 font-medium text-right"><%= highConf %></span>
              </div>
              <div class="flex items-center gap-3">
                <span class="w-20 text-xs text-gray-600">Medium</span>
                <div class="flex-1 bg-gray-200 rounded-full h-4 overflow-hidden">
                  <div class="bg-yellow-400 h-4 rounded-full transition-all" style="width: <%= relsWithConfidence.length > 0 ? (medConf / relsWithConfidence.length * 100) : 0 %>%"></div>
                </div>
                <span class="w-8 text-xs text-gray-700 font-medium text-right"><%= medConf %></span>
              </div>
              <div class="flex items-center gap-3">
                <span class="w-20 text-xs text-gray-600">Low</span>
                <div class="flex-1 bg-gray-200 rounded-full h-4 overflow-hidden">
                  <div class="bg-red-400 h-4 rounded-full transition-all" style="width: <%= relsWithConfidence.length > 0 ? (lowConf / relsWithConfidence.length * 100) : 0 %>%"></div>
                </div>
                <span class="w-8 text-xs text-gray-700 font-medium text-right"><%= lowConf %></span>
              </div>
              <div class="flex items-center gap-3">
                <span class="w-20 text-xs text-gray-600">Very Low</span>
                <div class="flex-1 bg-gray-200 rounded-full h-4 overflow-hidden">
                  <div class="bg-gray-400 h-4 rounded-full transition-all" style="width: <%= relsWithConfidence.length > 0 ? (veryLowConf / relsWithConfidence.length * 100) : 0 %>%"></div>
                </div>
                <span class="w-8 text-xs text-gray-700 font-medium text-right"><%= veryLowConf %></span>
              </div>
            </div>
            <div class="mt-4 pt-4 border-t border-gray-100 flex items-center justify-between">
              <span class="text-sm text-gray-600">Average Confidence</span>
              <span class="text-lg font-bold <%= avgScore >= 80 ? 'text-green-600' : avgScore >= 60 ? 'text-yellow-600' : avgScore >= 40 ? 'text-orange-600' : 'text-red-600' %>"><%= avgScore %>%</span>
            </div>
          </div>

          <!-- Low Confidence Relationships -->
          <div>
            <h3 class="text-sm font-medium text-gray-700 mb-3">
              <% if (lowConfRels.length > 0) { %>
              Relationships Needing Review (<%= lowConfRels.length %>)
              <% } else { %>
              All Relationships Look Good!
              <% } %>
            </h3>
            <% if (lowConfRels.length > 0) { %>
            <div class="space-y-2 max-h-48 overflow-y-auto">
              <% for (const rel of lowConfRels.slice(0, 10)) { %>
              <div class="flex items-center justify-between px-3 py-2 bg-gray-50 rounded-lg text-sm">
                <div class="flex items-center gap-1 text-gray-700 truncate">
                  <span class="font-medium"><%= rel.fromContainer %></span>
                  <span class="text-gray-400">.</span>
                  <span class="text-purple-600"><%= rel.fromProperty %></span>
                  <svg class="w-3 h-3 text-gray-400 mx-1" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 7l5 5m0 0l-5 5m5-5H6"/>
                  </svg>
                  <span><%= rel.toContainer %></span>
                </div>
                <span class="confidence-badge confidence-<%= rel.confidence.level %>"><%= rel.confidence.score %>%</span>
              </div>
              <% } %>
              <% if (lowConfRels.length > 10) { %>
              <div class="text-xs text-gray-500 text-center py-1">
                + <%= lowConfRels.length - 10 %> more
              </div>
              <% } %>
            </div>
            <% } else { %>
            <div class="flex items-center gap-2 px-3 py-4 bg-green-50 rounded-lg text-sm text-green-700">
              <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"/>
              </svg>
              All detected relationships have medium or high confidence scores.
            </div>
            <% } %>

            <% if (orphanRels.length > 0) { %>
            <div class="mt-4 pt-4 border-t border-gray-100">
              <h4 class="text-xs font-medium text-orange-700 mb-2">Orphan References (<%= orphanRels.length %>)</h4>
              <div class="text-xs text-gray-500">
                Properties that look like foreign keys but target containers that don't exist.
              </div>
              <div class="mt-2 flex flex-wrap gap-1">
                <% for (const rel of orphanRels.slice(0, 5)) { %>
                <span class="px-2 py-1 bg-orange-50 text-orange-700 rounded text-xs">
                  <%= rel.fromContainer %>.<%= rel.fromProperty %> → <span class="line-through"><%= rel.toContainer %></span>
                </span>
                <% } %>
                <% if (orphanRels.length > 5) { %>
                <span class="px-2 py-1 text-gray-500 text-xs">+ <%= orphanRels.length - 5 %> more</span>
                <% } %>
              </div>
            </div>
            <% } %>
          </div>
        </div>
      </div>
      <% } %>
    </section>

    <!-- ERD Diagram -->
    <section id="erd" class="bg-white rounded-xl shadow-sm border border-gray-200 p-6 mb-8">
      <h2 class="text-xl font-bold text-gray-900 mb-4 flex items-center gap-2">
        <svg class="w-5 h-5 text-cosmos-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 17V7m0 10a2 2 0 01-2 2H5a2 2 0 01-2-2V7a2 2 0 012-2h2a2 2 0 012 2m0 10a2 2 0 002 2h2a2 2 0 002-2M9 7a2 2 0 012-2h2a2 2 0 012 2m0 10V7m0 10a2 2 0 002 2h2a2 2 0 002-2V7a2 2 0 00-2-2h-2a2 2 0 00-2 2"/>
        </svg>
        Entity Relationship Diagram
      </h2>
      <div class="diagram-container" style="height: calc(100vh - 200px); min-height: 500px; max-height: 800px;">
        <div class="diagram-controls">
          <button onclick="zoomIn(this)" title="Zoom In">+</button>
          <button onclick="zoomOut(this)" title="Zoom Out">−</button>
          <button onclick="resetZoom(this)" title="Reset">⟲</button>
        </div>
        <div class="diagram-viewport">
          <pre class="mermaid">
<%- simpleERD %>
          </pre>
        </div>
      </div>
      <div class="mt-4 text-sm text-gray-500 flex flex-wrap gap-4">
        <span><code class="bg-gray-100 px-2 py-1 rounded">||</code> Exactly one</span>
        <span><code class="bg-gray-100 px-2 py-1 rounded">o|</code> Zero or one</span>
        <span><code class="bg-gray-100 px-2 py-1 rounded">}o</code> Zero or more</span>
        <span><code class="bg-gray-100 px-2 py-1 rounded">}|</code> One or more</span>
      </div>
    </section>

    <!-- Databases -->
    <% for (const [dbName, dbInfo] of Object.entries(databases)) { %>
    <details class="db-section bg-white rounded-xl shadow-sm border border-gray-200 mb-6 overflow-hidden" data-database="<%= dbName %>" open>
      <summary class="bg-gradient-to-r from-gray-50 to-white px-6 py-4 border-b border-gray-200 cursor-pointer hover:from-gray-100 hover:to-gray-50 transition-colors">
        <h2 class="text-xl font-bold text-gray-900 flex items-center gap-2">
          <svg class="w-4 h-4 chevron text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"/>
          </svg>
          <%= dbName %>
          <span class="text-sm font-normal text-gray-500">(<%= dbInfo.containers.length %> containers)</span>
        </h2>
      </summary>

      <div class="p-6">
        <!-- Simple ERD (relationship overview) -->
        <% if (databaseSimpleERDs[dbName]) { %>
        <% const dbIntraRels = relationships.filter(r => r.fromDatabase === dbName && r.toDatabase === dbName && !r.isOrphan); %>
        <% const dbCrossRels = relationships.filter(r => r.fromDatabase === dbName && r.toDatabase !== dbName && !r.isOrphan); %>
        <details class="mb-6" open>
          <summary class="flex items-center gap-2 text-sm font-medium text-gray-700 mb-3">
            <svg class="w-4 h-4 chevron text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"/>
            </svg>
            <%= dbName %> Relationships
            <% if (dbIntraRels.length > 0) { %>
            <span class="ml-2 px-2 py-0.5 text-xs font-medium bg-purple-100 text-purple-700 rounded-full">
              <%= dbIntraRels.length %> shown
            </span>
            <% } %>
            <% if (dbCrossRels.length > 0) { %>
            <span class="px-2 py-0.5 text-xs font-medium bg-orange-100 text-orange-700 rounded-full" title="Cross-database relationships - see container details">
              + <%= dbCrossRels.length %> cross-db
            </span>
            <% } %>
          </summary>
          <div class="diagram-container" style="height: 450px; min-height: 300px; max-height: 550px;">
            <div class="diagram-controls">
              <button onclick="zoomIn(this)" title="Zoom In">+</button>
              <button onclick="zoomOut(this)" title="Zoom Out">−</button>
              <button onclick="resetZoom(this)" title="Reset">⟲</button>
            </div>
            <div class="diagram-viewport">
              <pre class="mermaid">
<%- databaseSimpleERDs[dbName] %>
              </pre>
            </div>
          </div>
        </details>
        <% } %>

        <!-- Detailed ERD (with entity properties) -->
        <% if (databaseERDs[dbName]) { %>
        <details class="mb-6">
          <summary class="flex items-center gap-2 text-sm font-medium text-gray-700 mb-3">
            <svg class="w-4 h-4 chevron text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"/>
            </svg>
            <%= dbName %> Schema Details
          </summary>
          <div class="diagram-container" style="height: calc(100vh - 300px); min-height: 400px; max-height: 600px;">
            <div class="diagram-controls">
              <button onclick="zoomIn(this)" title="Zoom In">+</button>
              <button onclick="zoomOut(this)" title="Zoom Out">−</button>
              <button onclick="resetZoom(this)" title="Reset">⟲</button>
            </div>
            <div class="diagram-viewport">
              <pre class="mermaid">
<%- databaseERDs[dbName] %>
              </pre>
            </div>
          </div>
        </details>
        <% } %>

        <!-- Containers -->
        <% for (const containerName of dbInfo.containers) { %>
        <% const schema = containerSchemas[`${dbName}.${containerName}`] || containerSchemas[containerName]; %>
        <% if (schema) { %>
        <% const containerRels = relationships.filter(r => r.fromContainer === containerName && r.fromDatabase === dbName && !r.isOrphan); %>
        <% const hasCrossDb = containerRels.some(r => r.toDatabase !== dbName); %>
        <details id="<%= dbName %>-<%= containerName %>" class="container-details border border-gray-200 rounded-lg mb-4" data-container="<%= containerName %>" data-database="<%= dbName %>">
          <summary class="flex items-center justify-between px-4 py-3 bg-gray-50 hover:bg-gray-100 transition-colors rounded-t-lg">
            <div class="flex items-center gap-2">
              <svg class="w-4 h-4 chevron text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"/>
              </svg>
              <span class="font-semibold text-gray-900 container-name"><%= containerName %></span>
            </div>
            <div class="flex gap-2">
              <span class="px-2 py-1 text-xs font-medium bg-blue-100 text-blue-700 rounded-full">
                <%= getRootProperties(schema.properties).length %> properties
              </span>
              <% if (containerRels.length > 0) { %>
              <span class="px-2 py-1 text-xs font-medium <%= hasCrossDb ? 'bg-orange-100 text-orange-700' : 'bg-purple-100 text-purple-700' %> rounded-full" <%= hasCrossDb ? 'title="Includes cross-database relationships"' : '' %>>
                <%= containerRels.length %> rel<%= hasCrossDb ? ' ⚡' : '' %>
              </span>
              <% } %>
            </div>
          </summary>
          <div class="p-4">
            <!-- Properties Table -->
            <table class="min-w-full divide-y divide-gray-200 mb-4">
              <thead class="bg-gray-50">
                <tr>
                  <th class="px-4 py-2 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">Property</th>
                  <th class="px-4 py-2 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">Type</th>
                  <th class="px-4 py-2 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">Status</th>
                  <th class="px-4 py-2 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">Example</th>
                  <th class="px-4 py-2 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">Notes</th>
                </tr>
              </thead>
              <tbody class="bg-white divide-y divide-gray-200">
                <% for (const prop of getRootProperties(schema.properties)) { %>
                <tr class="hover:bg-gray-50 property-row" data-property="<%= prop.name.toLowerCase() %>">
                  <td class="px-4 py-2 text-sm font-medium text-gray-900">
                    <span class="property-name"><%= prop.name %></span>
                    <% if (prop.name === 'id') { %>
                    <span class="ml-1 px-1.5 py-0.5 text-xs font-medium bg-yellow-100 text-yellow-800 rounded">ID</span>
                    <% } %>
                    <% const propRel = containerRels.find(r => r.fromProperty === prop.name); %>
                    <% if (propRel) { %>
                    <% const isRefCrossDb = propRel.toDatabase !== dbName; %>
                    <span class="ml-1 px-1.5 py-0.5 text-xs font-medium <%= isRefCrossDb ? 'bg-orange-100 text-orange-800' : 'bg-purple-100 text-purple-800' %> rounded">REF</span>
                    <% } %>
                  </td>
                  <td class="px-4 py-2 text-sm text-gray-600 property-type">
                    <%= prop.types.map(t => getTypeDisplayName(t)).join(' | ') %>
                  </td>
                  <td class="px-4 py-2 text-sm">
                    <% const optionality = prop.optionality || (prop.isRequired ? 'required' : 'optional'); %>
                    <% if (optionality === 'required') { %>
                    <span class="inline-flex items-center px-2 py-0.5 rounded text-xs font-medium bg-green-100 text-green-800">Required</span>
                    <% } else if (optionality === 'nullable') { %>
                    <span class="inline-flex items-center px-2 py-0.5 rounded text-xs font-medium bg-yellow-100 text-yellow-800">Nullable</span>
                    <% } else if (optionality === 'optional') { %>
                    <span class="inline-flex items-center px-2 py-0.5 rounded text-xs font-medium bg-gray-100 text-gray-600">Optional</span>
                    <% } else { %>
                    <span class="inline-flex items-center px-2 py-0.5 rounded text-xs font-medium bg-gray-50 text-gray-400">Sparse</span>
                    <% } %>
                  </td>
                  <td class="px-4 py-2 text-sm text-gray-500 font-mono text-xs max-w-xs truncate">
                    <%= prop.examples && prop.examples[0] ? prop.examples[0] : '-' %>
                  </td>
                  <td class="px-4 py-2 text-sm text-gray-500 text-xs">
                    <% if (prop.isEnum && prop.enumValues) { %>
                    <span class="inline-flex items-center px-1.5 py-0.5 rounded text-xs font-medium bg-blue-100 text-blue-800 mr-1" title="<%= prop.enumValues.join(', ') %>">
                      Enum (<%= prop.enumValues.length %>)
                    </span>
                    <% } %>
                    <% if (prop.isComputed) { %>
                    <span class="inline-flex items-center px-1.5 py-0.5 rounded text-xs font-medium bg-indigo-100 text-indigo-800" title="<%= prop.computedPattern %>">
                      Computed
                    </span>
                    <% } %>
                    <% if (!prop.isEnum && !prop.isComputed) { %>
                    -
                    <% } %>
                  </td>
                </tr>
                <% } %>
              </tbody>
            </table>

            <!-- Relationships -->
            <% if (containerRels.length > 0) { %>
            <div class="mt-4 pt-4 border-t border-gray-200">
              <h4 class="text-sm font-medium text-gray-700 mb-2">Outgoing Relationships</h4>
              <div class="space-y-2">
                <% for (const rel of containerRels) { %>
                <% const isRelCrossDb = rel.toDatabase !== dbName; %>
                <% const confLevel = rel.confidence?.level || 'medium'; %>
                <% const confScore = rel.confidence?.score || 50; %>
                <% const factors = rel.confidence?.factors || {}; %>
                <% const cardType = factors.cardinality?.type; %>
                <% const cardLabel = cardType === 'one-to-one' ? '1:1' : cardType === 'many-to-one' ? 'N:1' : ''; %>
                <details class="relationship-detail border border-gray-200 rounded-lg overflow-hidden">
                  <summary class="relationship-badge flex items-center justify-between px-3 py-2 cursor-pointer hover:bg-gray-50 <%= isRelCrossDb ? 'bg-orange-50/50' : 'bg-purple-50/50' %>"
                       data-from-property="<%= rel.fromProperty.toLowerCase() %>"
                       data-to-container="<%= rel.toContainer.toLowerCase() %>">
                    <div class="flex items-center text-sm <%= isRelCrossDb ? 'text-orange-700' : 'text-purple-700' %>">
                      <svg class="w-3 h-3 chevron text-gray-400 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"/>
                      </svg>
                      <span class="font-medium"><%= rel.fromProperty %></span>
                      <svg class="w-4 h-4 mx-1" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 7l5 5m0 0l-5 5m5-5H6"/>
                      </svg>
                      <span><%= rel.toContainer %></span>
                      <% if (isRelCrossDb) { %>
                      <span class="ml-1 text-xs text-orange-500">(<%= rel.toDatabase %>)</span>
                      <% } %>
                    </div>
                    <div class="flex items-center gap-1">
                      <% if (cardLabel) { %>
                      <span class="px-1.5 py-0.5 text-xs font-medium bg-gray-200 text-gray-700 rounded"><%= cardLabel %></span>
                      <% } %>
                      <% if (factors.denormalization?.isDenormalized === true) { %>
                      <span class="px-1.5 py-0.5 text-xs font-medium bg-blue-100 text-blue-700 rounded" title="Denormalized data detected">SNAP</span>
                      <% } %>
                      <span class="confidence-badge confidence-<%= confLevel %>"><%= confScore %>%</span>
                    </div>
                  </summary>
                  <div class="px-4 py-3 bg-white border-t border-gray-100 text-sm">
                    <div class="grid grid-cols-1 md:grid-cols-2 gap-3">
                      <!-- Referential Integrity -->
                      <div class="flex items-start gap-2">
                        <div class="w-5 h-5 mt-0.5 rounded-full flex items-center justify-center text-xs font-bold <%= factors.referentialIntegrity?.confidence >= 70 ? 'bg-green-100 text-green-700' : factors.referentialIntegrity?.confidence >= 40 ? 'bg-yellow-100 text-yellow-700' : 'bg-gray-100 text-gray-500' %>">
                          <% if (factors.referentialIntegrity?.confidence >= 70) { %>✓<% } else if (factors.referentialIntegrity?.confidence >= 40) { %>~<% } else { %>?<% } %>
                        </div>
                        <div>
                          <div class="font-medium text-gray-700">Referential Integrity</div>
                          <div class="text-xs text-gray-500">
                            <% if (factors.referentialIntegrity?.matchRate !== undefined) { %>
                            <%= Math.round(factors.referentialIntegrity.matchRate * 100) %>% match (<%= factors.referentialIntegrity.sampleSize || 0 %> sampled)
                            <% } else { %>
                            <%= factors.referentialIntegrity?.reason || 'Not validated' %>
                            <% } %>
                          </div>
                        </div>
                      </div>
                      <!-- Type Consistency -->
                      <div class="flex items-start gap-2">
                        <div class="w-5 h-5 mt-0.5 rounded-full flex items-center justify-center text-xs font-bold <%= factors.typeConsistency?.consistent ? 'bg-green-100 text-green-700' : 'bg-red-100 text-red-700' %>">
                          <% if (factors.typeConsistency?.consistent) { %>✓<% } else { %>✗<% } %>
                        </div>
                        <div>
                          <div class="font-medium text-gray-700">Type Consistency</div>
                          <div class="text-xs text-gray-500">
                            <% if (factors.typeConsistency?.fkTypes && factors.typeConsistency?.idTypes) { %>
                            <%= factors.typeConsistency.fkTypes.join('/') %> → <%= factors.typeConsistency.idTypes.join('/') %>
                            <% } else { %>
                            <%= factors.typeConsistency?.reason || 'Unknown' %>
                            <% } %>
                          </div>
                        </div>
                      </div>
                      <!-- Frequency -->
                      <div class="flex items-start gap-2">
                        <div class="w-5 h-5 mt-0.5 rounded-full flex items-center justify-center text-xs font-bold <%= factors.frequency?.confidence >= 70 ? 'bg-green-100 text-green-700' : factors.frequency?.confidence >= 40 ? 'bg-yellow-100 text-yellow-700' : 'bg-gray-100 text-gray-500' %>">
                          <% if (factors.frequency?.populatedRate >= 0.7) { %>✓<% } else if (factors.frequency?.populatedRate >= 0.3) { %>~<% } else { %>○<% } %>
                        </div>
                        <div>
                          <div class="font-medium text-gray-700">Field Frequency</div>
                          <div class="text-xs text-gray-500">
                            <% if (factors.frequency?.populatedRate !== undefined) { %>
                            <%= Math.round(factors.frequency.populatedRate * 100) %>% populated
                            <% } else { %>
                            <%= factors.frequency?.reason || 'Unknown' %>
                            <% } %>
                          </div>
                        </div>
                      </div>
                      <!-- Naming Pattern -->
                      <div class="flex items-start gap-2">
                        <div class="w-5 h-5 mt-0.5 rounded-full flex items-center justify-center text-xs font-bold <%= factors.namingPattern?.confidence >= 80 ? 'bg-green-100 text-green-700' : factors.namingPattern?.confidence >= 60 ? 'bg-yellow-100 text-yellow-700' : 'bg-gray-100 text-gray-500' %>">
                          <% if (factors.namingPattern?.confidence >= 80) { %>✓<% } else if (factors.namingPattern?.confidence >= 60) { %>~<% } else { %>?<% } %>
                        </div>
                        <div>
                          <div class="font-medium text-gray-700">Naming Pattern</div>
                          <div class="text-xs text-gray-500"><%= factors.namingPattern?.pattern || 'unclear' %></div>
                        </div>
                      </div>
                      <% if (factors.denormalization) { %>
                      <!-- Denormalization -->
                      <div class="flex items-start gap-2">
                        <div class="w-5 h-5 mt-0.5 rounded-full flex items-center justify-center text-xs font-bold <%= factors.denormalization?.isDenormalized === true ? 'bg-blue-100 text-blue-700' : 'bg-gray-100 text-gray-500' %>">
                          <% if (factors.denormalization?.isDenormalized === true) { %>◐<% } else { %>○<% } %>
                        </div>
                        <div>
                          <div class="font-medium text-gray-700">Denormalization</div>
                          <div class="text-xs text-gray-500">
                            <% if (factors.denormalization?.isDenormalized === true) { %>
                            Snapshot: <%= factors.denormalization.nestedFields?.join(', ') || 'embedded' %>
                            <% } else if (factors.denormalization?.isDenormalized === 'possible') { %>
                            Possibly embedded data
                            <% } else { %>
                            Live reference (no snapshot)
                            <% } %>
                          </div>
                        </div>
                      </div>
                      <% } %>
                      <% if (factors.cardinality) { %>
                      <!-- Cardinality -->
                      <div class="flex items-start gap-2">
                        <div class="w-5 h-5 mt-0.5 rounded-full flex items-center justify-center text-xs font-bold bg-indigo-100 text-indigo-700">
                          <% if (factors.cardinality?.type === 'one-to-one') { %>1<% } else if (factors.cardinality?.type === 'many-to-one') { %>N<% } else { %>?<% } %>
                        </div>
                        <div>
                          <div class="font-medium text-gray-700">Cardinality</div>
                          <div class="text-xs text-gray-500">
                            <% if (factors.cardinality?.type === 'one-to-one') { %>
                            1:1 (single reference per target)
                            <% } else if (factors.cardinality?.type === 'many-to-one') { %>
                            N:1 (avg <%= factors.cardinality.avgReferences?.toFixed(1) || '?' %>, max <%= factors.cardinality.maxReferences || '?' %>)
                            <% } else { %>
                            <%= factors.cardinality?.error || 'Unknown' %>
                            <% } %>
                          </div>
                        </div>
                      </div>
                      <% } %>
                    </div>
                    <% if (rel.confidence?.summary) { %>
                    <div class="mt-3 pt-3 border-t border-gray-100 text-xs text-gray-600">
                      <strong>Summary:</strong> <%= rel.confidence.summary %>
                    </div>
                    <% } %>
                  </div>
                </details>
                <% } %>
              </div>
            </div>
            <% } %>
          </div>
        </details>
        <% } %>
        <% } %>
      </div>
    </details>
    <% } %>

    <!-- Cross-Database Relationships -->
    <% const crossDbRels = relationships.filter(r => r.isCrossDatabase && !r.isOrphan); %>
    <% if (crossDbRels.length > 0) { %>
    <section class="bg-white rounded-xl shadow-sm border border-gray-200 p-6 mb-8">
      <h2 class="text-xl font-bold text-gray-900 mb-4 flex items-center gap-2">
        <svg class="w-5 h-5 text-orange-500" fill="none" stroke="currentColor" viewBox="0 0 24 24">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M8 7h12m0 0l-4-4m4 4l-4 4m0 6H4m0 0l4 4m-4-4l4-4"/>
        </svg>
        Cross-Database Relationships
      </h2>
      <div class="overflow-x-auto">
        <table class="min-w-full divide-y divide-gray-200">
          <thead class="bg-gray-50">
            <tr>
              <th class="px-4 py-2 text-left text-xs font-medium text-gray-500 uppercase">Source</th>
              <th class="px-4 py-2 text-left text-xs font-medium text-gray-500 uppercase">Property</th>
              <th class="px-4 py-2 text-left text-xs font-medium text-gray-500 uppercase">Target</th>
              <th class="px-4 py-2 text-left text-xs font-medium text-gray-500 uppercase">Type</th>
              <th class="px-4 py-2 text-left text-xs font-medium text-gray-500 uppercase">Confidence</th>
            </tr>
          </thead>
          <tbody class="divide-y divide-gray-200">
            <% for (const rel of crossDbRels) { %>
            <% const confLevel = rel.confidence?.level || 'medium'; %>
            <% const confScore = rel.confidence?.score || 50; %>
            <% const cardType = rel.confidence?.factors?.cardinality?.type; %>
            <% const cardDisplay = cardType === 'one-to-one' ? '1:1' : cardType === 'many-to-one' ? 'N:1' : rel.cardinality; %>
            <tr class="hover:bg-gray-50" title="<%= rel.confidence?.summary || '' %>">
              <td class="px-4 py-2 text-sm">
                <span class="font-medium text-gray-900"><%= rel.fromDatabase %></span>.<%= rel.fromContainer %>
              </td>
              <td class="px-4 py-2 text-sm font-mono text-purple-600"><%= rel.fromProperty %></td>
              <td class="px-4 py-2 text-sm">
                <span class="font-medium text-gray-900"><%= rel.toDatabase %></span>.<%= rel.toContainer %>
              </td>
              <td class="px-4 py-2 text-sm text-gray-500"><%= cardDisplay %></td>
              <td class="px-4 py-2 text-sm">
                <span class="confidence-badge confidence-<%= confLevel %>"><%= confScore %>%</span>
              </td>
            </tr>
            <% } %>
          </tbody>
        </table>
      </div>
    </section>
    <% } %>
  </main>

  <!-- Footer -->
  <footer class="main-content bg-gray-100 border-t border-gray-200 mt-12">
    <div class="px-4 py-6 sm:px-6 lg:px-8">
      <p class="text-center text-sm text-gray-500">
        Generated by <a href="https://github.com/afrugalpenguin/cosmos-mapper" class="text-cosmos-600 hover:underline">CosmosMapper</a>
      </p>
    </div>
  </footer>

  <script>
    // Initialize Mermaid
    mermaid.initialize({
      startOnLoad: true,
      theme: 'base',
      themeVariables: {
        primaryColor: '#dbeafe',
        primaryTextColor: '#1e40af',
        primaryBorderColor: '#3b82f6',
        lineColor: '#6b7280',
        secondaryColor: '#f3f4f6',
        tertiaryColor: '#fff'
      }
    });

    // Store panzoom instances
    const panzoomInstances = new WeakMap();

    // Initialize panzoom on all diagram viewports after Mermaid renders
    document.addEventListener('DOMContentLoaded', () => {
      setTimeout(() => {
        document.querySelectorAll('.diagram-viewport').forEach(viewport => {
          const instance = panzoom(viewport, {
            maxZoom: 5,
            minZoom: 0.25,
            initialZoom: 1,
            bounds: true,
            boundsPadding: 0.1
          });
          panzoomInstances.set(viewport, instance);
        });
      }, 500);
    });

    function getViewport(button) {
      return button.closest('.diagram-container').querySelector('.diagram-viewport');
    }

    function zoomIn(button) {
      const viewport = getViewport(button);
      const instance = panzoomInstances.get(viewport);
      if (instance) {
        const transform = instance.getTransform();
        instance.zoomAbs(viewport.clientWidth / 2, viewport.clientHeight / 2, transform.scale * 1.3);
      }
    }

    function zoomOut(button) {
      const viewport = getViewport(button);
      const instance = panzoomInstances.get(viewport);
      if (instance) {
        const transform = instance.getTransform();
        instance.zoomAbs(viewport.clientWidth / 2, viewport.clientHeight / 2, transform.scale / 1.3);
      }
    }

    function resetZoom(button) {
      const viewport = getViewport(button);
      const instance = panzoomInstances.get(viewport);
      if (instance) {
        instance.moveTo(0, 0);
        instance.zoomAbs(0, 0, 1);
      }
    }

    // Sidebar toggle
    function toggleSidebar() {
      document.getElementById('sidebar').classList.toggle('open');
      document.getElementById('sidebarOverlay').classList.toggle('open');
    }

    // Expand/Collapse all
    function expandAll() {
      document.querySelectorAll('.container-details, .nav-db-section, .db-section').forEach(d => d.open = true);
    }

    function collapseAll() {
      document.querySelectorAll('.container-details, .nav-db-section, .db-section').forEach(d => d.open = false);
    }

    // Search functionality
    let searchTimeout;
    function handleSearch(query) {
      clearTimeout(searchTimeout);
      searchTimeout = setTimeout(() => performSearch(query), 150);
    }

    function performSearch(query) {
      const clearBtn = document.getElementById('clearSearchBtn');
      const resultsDiv = document.getElementById('searchResults');

      if (!query.trim()) {
        clearBtn.classList.add('hidden');
        resultsDiv.classList.add('hidden');
        // Reset all visibility
        document.querySelectorAll('.container-details').forEach(el => {
          el.classList.remove('search-hidden', 'search-match');
        });
        document.querySelectorAll('.property-row').forEach(el => {
          el.classList.remove('search-hidden', 'search-match');
        });
        document.querySelectorAll('.nav-item').forEach(el => {
          el.classList.remove('search-hidden');
        });
        // Remove highlights
        document.querySelectorAll('.search-highlight').forEach(el => {
          el.outerHTML = el.textContent;
        });
        // Reset relationship badges
        document.querySelectorAll('.relationship-badge').forEach(el => {
          el.classList.remove('search-match');
        });
        return;
      }

      clearBtn.classList.remove('hidden');
      const lowerQuery = query.toLowerCase();
      let matchCount = 0;
      let containerMatches = 0;
      let relationshipMatches = 0;

      // Search containers and properties
      document.querySelectorAll('.container-details').forEach(container => {
        const containerName = container.dataset.container.toLowerCase();
        let hasMatch = containerName.includes(lowerQuery);

        // Search properties within container
        container.querySelectorAll('.property-row').forEach(row => {
          const propName = row.dataset.property;
          const propType = row.querySelector('.property-type')?.textContent.toLowerCase() || '';

          if (propName.includes(lowerQuery) || propType.includes(lowerQuery)) {
            row.classList.remove('search-hidden');
            row.classList.add('search-match');
            hasMatch = true;
            matchCount++;
          } else {
            row.classList.add('search-hidden');
            row.classList.remove('search-match');
          }
        });

        // Search relationships within container
        container.querySelectorAll('.relationship-badge').forEach(badge => {
          const fromProp = badge.dataset.fromProperty || '';
          const toContainer = badge.dataset.toContainer || '';

          if (fromProp.includes(lowerQuery) || toContainer.includes(lowerQuery)) {
            badge.classList.add('search-match');
            hasMatch = true;
            relationshipMatches++;
          } else {
            badge.classList.remove('search-match');
          }
        });

        if (hasMatch) {
          container.classList.remove('search-hidden');
          container.classList.add('search-match');
          container.open = true;
          containerMatches++;
        } else {
          container.classList.add('search-hidden');
          container.classList.remove('search-match');
        }
      });

      // Update nav items
      document.querySelectorAll('.nav-item[data-container]').forEach(navItem => {
        const containerName = navItem.dataset.container.toLowerCase();
        if (containerName.includes(lowerQuery)) {
          navItem.classList.remove('search-hidden');
        } else {
          navItem.classList.add('search-hidden');
        }
      });

      // Show results count
      resultsDiv.classList.remove('hidden');
      resultsDiv.textContent = `${containerMatches} container${containerMatches !== 1 ? 's' : ''}, ${matchCount} propert${matchCount !== 1 ? 'ies' : 'y'}, ${relationshipMatches} relationship${relationshipMatches !== 1 ? 's' : ''}`;
    }

    function clearSearch() {
      document.getElementById('searchInput').value = '';
      performSearch('');
    }

    // Highlight active nav item on scroll
    const observer = new IntersectionObserver((entries) => {
      entries.forEach(entry => {
        if (entry.isIntersecting) {
          const id = entry.target.id;
          document.querySelectorAll('.nav-item').forEach(nav => {
            nav.classList.remove('active');
            if (nav.getAttribute('href') === '#' + id) {
              nav.classList.add('active');
            }
          });
        }
      });
    }, { threshold: 0.3 });

    document.querySelectorAll('[id]').forEach(el => {
      if (el.id) observer.observe(el);
    });

    // Keyboard shortcut for search
    document.addEventListener('keydown', (e) => {
      if ((e.ctrlKey || e.metaKey) && e.key === 'k') {
        e.preventDefault();
        document.getElementById('searchInput').focus();
      }
      if (e.key === 'Escape') {
        clearSearch();
        document.getElementById('searchInput').blur();
      }
    });
  </script>
</body>
</html>
